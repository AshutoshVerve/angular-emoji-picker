{
  "rules": {
    "curly": [
      2,
      "all"
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "no-with": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": 2,
    "one-var": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "wrap-iife": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-multi-str": 2,
    "no-implicit-coercion": [
      2,
      {
        "boolean": true,
        "string": true,
        "number": true
      }
    ],
    "yoda": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true
      }
    ],
    "dot-notation": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "max-len": [
      2,
      120
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "eol-last": 2,
    "no-multiple-empty-lines": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}
